<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class Coupon extends Model
{
    use HasFactory;

    protected $table = 'coupons';

    protected $fillable = [
        'CouponId',
        'Code',
        'Description',
        'Start_Date',
        'End_Date',
    ];

    protected $casts = [
        'Start_Date' => 'datetime',
        'End_Date' => 'datetime',
    ];
}
